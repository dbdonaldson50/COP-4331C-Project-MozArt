<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.thefirstorder.mozart">
    <!-- This is for creating the MozArt Folder -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Startup">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomePage" />
        <activity android:name=".About" />
        <activity
            android:name=".options"
            android:label="@string/title_activity_options" />
        <activity android:name=".images" />
        <activity
            android:name=".MusicLibrary"
            android:label="@string/title_activity_music_library"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".Player"
            android:label="Songs"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
